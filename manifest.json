{
  "manifest_version": 3,
  "name": "History Auto-Delete",
  "author": "Collin Grimm <hello@collingrimm.com>",
  "version": "1.0",
  "description": "Automatically deletes new browser history entries",
  "permissions": [
    "history",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "History Auto-Delete"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}